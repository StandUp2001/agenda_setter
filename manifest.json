{
  "manifest_version": 3,
  "name": "Agenda Setter",
  "description": "Sets info for your agenda",
  "version": "1.0",
  "action": {
    "default_title": "Agenda Setter",
    "default_icon": {
      "16": "./icons/16.png",
      "32": "./icons/32.png",
      "48": "./icons/48.png",
      "128": "./icons/128.png"
    }
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "128": "./icons/128.png"
  },
  "background": {
    "service_worker": "./background.js",
    "type": "module"
  },
  "host_permissions": [
    "https://calendar.google.com/*"
  ],
  "permissions": [
    "tabs",
    "scripting"
  ]
}